@model MVCExample.Models.ViewModel.BasketViewModel

@{
    ViewData["Title"] = "View";
}

<h2>View</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.BasketItems) {
        <tr>
        
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td>£@item.Price</td>
            <td>@item.ProductType.Name</td>
        
        </tr>
}
    </tbody>
</table>
